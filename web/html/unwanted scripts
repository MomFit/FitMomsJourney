<!-- dashboard -->

<div id="container"></div>
  
<!-- 
	// data for the charts 
	  // [drinksData -> exercisesData]
	  /* The exercisesData variable contains a data set for a fitness dashboard that tracks different 
	  	 types of exercises and their associated metrics. The data is structured as follows:

			The first column contains the name of the exercise (as a string)
			The second column contains the percentage of users who reported that they enjoy the exercise (as a number)
			The third column contains the percentage of users who reported that they find the exercise challenging (as a number)
			The fourth column contains the percentage of users who reported that they have seen improvements in their fitness levels due to the exercise (as a number)
			The fifth column contains the percentage of users who reported that the exercise is part of their regular fitness routine (as a number)
			The sixth column contains the average number of minutes that users spend on the exercise per session (as a number)
			The six types of exercises and their order in the data are:

			Yoga
			Running
			Swimming
			Cycling
			Strength training
			Walking 
			
		Here is a breakdown of the data for each exercise:

			- Yoga: 20% of users enjoy it, 60% find it challenging, 10% have seen improvements, 5% do it regularly, and the average session lasts 200 minutes.
			- Running: 45% of users enjoy it, 40% find it challenging, 15% have seen improvements, 7% do it regularly, and the average session lasts 150 minutes.
			- Swimming: 30% of users enjoy it, 55% find it challenging, 25% have seen improvements, 8% do it regularly, and the average session lasts 100 minutes.
			- Cycling: 35% of users enjoy it, 30% find it challenging, 40% have seen improvements, 9% do it regularly, and the average session lasts 120 minutes.
			- Strength training: 50% of users enjoy it, 15% find it challenging, 35% have seen improvements, 6% do it regularly, and the average session lasts 180 minutes.
			- Walking: 25% of users enjoy it, 50% find it challenging, 25% have seen improvements, 4% do it regularly, and the average session lasts 250 minutes.
			*/
 -->
  <script>

    anychart.onDocumentReady(function () {
	// set chart theme
anychart.theme('lightEarth');
      // variable to help us define screen proportions
      var screenRatio;

      var exercisesData = anychart.data.set([
			['Yoga', 20, 60, 10, 5, 200],
			['Running', 45, 40, 15, 7, 150],
			['Swimming', 30, 55, 25, 8, 100],
			['Cycling', 35, 30, 40, 9, 120],
			['Strength Training', 50, 15, 35, 6, 180],
			['Walking', 25, 50, 25, 4, 250]
		]);

	  // [previousYearsData -> previousYearsData]
	  // The previousYearsData variable contains a data set for a fitness dashboard. 
	  // The data represents the average number of minutes spent on four types of exercises per day for 
	  // the years 2010 to 2015. The four types of exercises and their order in the data are:

			/* Cardio
			Strength training
			Stretching
			Yoga
			
			The data is structured as follows:

			The first column contains the year (as a string)
			The second column contains the average number of minutes spent on cardio exercise per day (as a number)
			The third column contains the average number of minutes spent on strength training per day (as a number)
			The fourth column contains the average number of minutes spent on stretching per day (as a number)
			The fifth column contains the average number of minutes spent on yoga per day (as a number) */

      var previousYearsData = anychart.data.set([
		['2010', 60, 176, 35, 25],
		['2011', 51, 136, 25, 26],
		['2012', 73, 196, 35, 38],
		['2013', 84, 315, 43, 41],
		['2014', 79, 203, 36, 33],
		['2015', 89, 286, 41, 48]
		]);

      // set default using the theme
      var dashboardTheme = {
        chart: {
          margin: 0,
          padding: [15, 30, 15, 15],
          title: {
            enabled: true,
            fontColor: '#212121',
            fontSize: 13,
            padding: [0, 0, 10, 0]
          },
          defaultXAxisSettings: {
            title: {
              enabled: false,
              fontSize: 12,
              margin: 0,
              padding: [3, 0, 0, 0]
            },
            labels: { fontSize: 11, padding: [0, 0, 0, 0] }
          },
          defaultYAxisSettings: {
            title: {
              enabled: true,
              fontSize: 12,
              margin: 0,
              padding: [0, 0, 3, 0]
            },
            labels: { fontSize: 11, padding: [0, 0, 0, 0] }
          }
        }
      };

      // apply the theme
      anychart.theme(dashboardTheme);

      // create stage for all charts
      var stage = acgraph.create('container');

      var title = anychart.standalones.title();
      title.text('Post Partum Women Exercise statistics');
      title.container(stage).draw();

      var pieChart = anychart.pie(exercisesData);
      pieChart.title('Exercise');
      pieChart.legend().enabled(false);
      pieChart.labels().position('outside').fontSize(10).format('{%x}');
      pieChart.tooltip().format('{%Value}% of user enjoy {%x}');
      pieChart.container(stage).draw();
	
      var barChart = anychart.bar();
      barChart.title('Exercises by % who enjoy and find it challenging');
      barChart.bar(drinksData.mapAs({ value: 3, x: 0 })).name('% of people find it challenging');
      barChart.bar(drinksData.mapAs({ value: 2, x: 0 })).name('% of people who enjoy');
      barChart.yAxis().title('% of people from the pie chart');
      barChart.container(stage).draw();

      var scatterChart = anychart.bubble();
      scatterChart.title('Improvements, Regularisation and Duration spent');
      scatterChart.bubble(
        drinksData.mapAs({
          value: 4,
          x: 5,
          size: 1,
          name: 0
        })
      );
      scatterChart.tooltip().titleFormat('{%Name}');
      scatterChart
        .tooltip()
        .format('% of people who sawe improvements: {%value}\n% of people who made this exercise regular: {%x}.\nAvg Duration spent: {%size} minutes');
      scatterChart.yScale().minimum(0).maximum(15);
      scatterChart.yAxis().title('Volume (ml)');
      scatterChart.xAxis().title('Price, $');
      scatterChart.container(stage).draw();

      var lineChart = anychart.line(
        previousYearsData.mapAs({ value: 1, x: 0 })
      );
      lineChart.title('Average duration in minutes spent in CARDIO per woman');
      lineChart.tooltip().format('{%value} minutes');
      lineChart.yAxis().title('Duration in minutes');
      lineChart.container(stage).draw();

      var areaChart = anychart.area(
        previousYearsData.mapAs({ value: 2, x: 0 })
      );

      areaChart.title('Average duration in minutes spent in STRENGTH training per woman');
      areaChart.tooltip().format('{%value} minutes');
      areaChart.yAxis().title('Duration in minutes');
      areaChart.container(stage).draw();

      var columnChart = anychart.column();
      columnChart.title('Average duration in minutes spent in STRETCHING & YOGA per woman');
      columnChart.yAxis().title('Duration in minutes');
      columnChart
        .column(previousYearsData.mapAs({ value: 3, x: 0 }))
        .name('stretching');
      columnChart
        .column(previousYearsData.mapAs({ value: 4, x: 0 }))
        .name('yoga');
      columnChart.yAxis().title('Duration in minutes');
      columnChart.container(stage).draw();

      // chart placement
      function configureChartsSize() {
        if (window.innerWidth > 768) {
          screenRatio = 'wide';
          pieChart.bounds(0, 30, '33.3%', '47%');
          barChart.bounds('33.3%', 30, '33.3%', '47%');
          scatterChart.bounds('66.6%', 30, '33.3%', '47%');
          lineChart.bounds(0, '52%', '33.3%', '47%');
          areaChart.bounds('33.3%', '52%', '33.3%', '47%');
          columnChart.bounds('66.6%', '52%', '33.3%', '47%');
        } else {
          screenRatio = 'slim';
          pieChart.bounds(0, 30, '50%', '33.3%');
          barChart.bounds('50%', 30, '50%', '33.3%');
          scatterChart.bounds(0, '33.3%', '50%', '33.3%');
          lineChart.bounds('50%', '33.3%', '50%', '33.3%');
          areaChart.bounds(0, '66.6%', '50%', '33.3%');
          columnChart.bounds('50%', '66.6%', '50%', '33.3%');
        }
      }

      configureChartsSize();

      // On resize change layout to mobile version or otherwise
      window.onresize = function () {
        if (screenRatio === 'slim' && window.innerWidth > 767) {
          configureChartsSize('wide');
        } else if (window.innerWidth <= 767) {
          configureChartsSize('slim');
        }
        title.parentBounds(0, 0, stage.width(), 130).draw();
      };
    });
  
</script>